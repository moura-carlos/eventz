<h1>Sign In</h1>
<%# by default, the form will post so we
don't have to specify the HTTP verb here. %>
<%= form_with(url: session_path) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, autofocus: true %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Sign In" %>
<% end %>
